{
  "name": "agency-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "build": "npm run validate:dtos && tsc -p tsconfig.build.json",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "seed": "ts-node -r tsconfig-paths/register src/seed/seed.ts",
    "seed:landing": "ts-node -r tsconfig-paths/register src/seed/landing-seed.ts",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "emit:dto": "ts-node -r tsconfig-paths/register src/scripts/emit_dto_samples.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "validate:dtos": "ts-node -r tsconfig-paths/register scripts/dto-validation/validate-dtos.ts",
    "validate:dtos:fix": "ts-node -r tsconfig-paths/register scripts/dto-validation/fix-dtos.ts",
    "validate:controllers": "ts-node -r tsconfig-paths/register scripts/dto-validation/validate-controllers.ts",
    "dto:metrics": "ts-node -r tsconfig-paths/register scripts/dto-validation/dto-metrics.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/core": "^10.4.7",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^7.3.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "firebase-admin": "^13.6.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/testing": "^10.4.7",
    "@nestjs/typeorm": "^11.0.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@typescript-eslint/utils": "^8.48.0",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "fast-glob": "^3.3.3",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-morph": "^27.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
